{
  "name": "Organic Scroll",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An extension allowing to make keyboard scrolling more pleasant.",
  "homepage_url": "https://github.com/lssol/organicScroll",
  "icons": {
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "https://*/*"
  ]
}